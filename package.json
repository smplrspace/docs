{
  "name": "smplrspace-docs",
  "version": "0.1.0",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "BROWSER='Google Chrome Dev' docusaurus start",
    "build": "docusaurus build && yarn check-deadlinks",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "format": "prettier-eslint --write \"src/**/*.{js,json,css}\"",
    "check-deadlinks": "yarn hyperlink build/ --check-anchors --sources docs/"
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css}": [
        "prettier-eslint --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.7",
    "@docusaurus/preset-classic": "2.0.0-beta.7",
    "@easyops-cn/docusaurus-search-local": "^0.19.1",
    "@mantine/core": "^3.0.5",
    "@mantine/hooks": "^3.0.5",
    "@mdx-js/react": "^1.6.21",
    "@smplrspace/smplr-loader": "^2.14.2-beta.4",
    "@stackblitz/sdk": "^1.8.2",
    "@svgr/webpack": "^5.5.0",
    "chance": "^1.1.8",
    "change-case": "^4.1.2",
    "chroma-js": "^2.1.2",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "prism-react-renderer": "^1.2.1",
    "ramda": "^0.27.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.3.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.62",
    "@untitaker/hyperlink": "^0.1.32",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
